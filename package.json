{
  "name": "@neovici/cfg",
  "version": "1.13.1",
  "description": "Configuration for Neovici packages",
  "homepage": "https://github.com/Neovici/cfg#readme",
  "bugs": {
    "url": "https://github.com/Neovici/cfg/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Neovici/cfg.git"
  },
  "license": "Apache-2.0",
  "author": "Neovici dev",
  "main": "index.js",
  "files": [
    "eslint/*.js",
    "prettier/*.js",
    "web/*.js",
    "web/*.mjs",
    "*.js"
  ],
  "scripts": {
    "test:eslint": "eslint *.js eslint/*.js",
    "test": "npm run test:eslint",
    "prepare": "husky install"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "branch": "master"
  },
  "publishConfig": {
    "access": "public"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.18.0",
    "@babel/eslint-parser": "^7.16.0",
    "@web/test-runner": "^0.13.27",
    "@web/test-runner-playwright": "^0.8.8",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.0",
    "eslint-plugin-mocha": "^10.0.0",
    "prettier": "^2.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "husky": "^8.0.0",
    "semantic-release": "^19.0.0"
  }
}
