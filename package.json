{
  "name": "@neovici/cfg",
  "version": "2.10.1",
  "description": "Configuration for Neovici packages",
  "homepage": "https://github.com/Neovici/cfg#readme",
  "bugs": {
    "url": "https://github.com/Neovici/cfg/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Neovici/cfg.git"
  },
  "license": "Apache-2.0",
  "author": "Neovici dev",
  "main": "index.js",
  "bin": {
    "check-duplicate-components": "./check-duplicate-components.mjs"
  },
  "files": [
    "eslint/*.js",
    "eslint/*.mjs",
    "prettier/*.js",
    "prettier/*.mjs",
    "web/*.js",
    "web/*.mjs",
    "*.js",
    "*.mjs"
  ],
  "scripts": {
    "lint": "eslint --cache .",
    "prepare": "husky"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.40.1",
    "@web/dev-server": "^0.4.0",
    "@web/dev-server-esbuild": "^1.0.0",
    "@web/test-runner": "^0.20.0",
    "@web/test-runner-commands": "^0.9.0",
    "@web/test-runner-playwright": "^0.11.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-mocha": "^11.2.0",
    "globals": "^17.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "^5.1.0",
    "typescript-eslint": "^8.53.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "husky": "^9.0.0",
    "semantic-release": "^25.0.0"
  }
}
